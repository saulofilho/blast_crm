<h2>New Contact</h2>
<hr>

<%= form_for([blast, @contact]) do |f| %>
  <%= render 'form', f: f %>

  <div class="form-group">
    <div>
        <%= f.submit "Create Contact", class: "btn btn-primary" %>
        <%= link_to 'Back', blast.contacts_path, class: 'btn btn-default' %>
    </div>
  </div>
<% end %>
