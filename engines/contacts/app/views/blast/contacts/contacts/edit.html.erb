<h2>Editing Contact</h2>
<hr>

<%= form_for([blast, @contact]) do |f| %>
  <%= render 'form', f: f %>
  <div class="form-group">
    <div>
      <%= f.submit "Update Contact", class: "btn btn-primary" %>
      <%= link_to 'Back', blast.contacts_path, class: 'btn btn-default' %>
    </div>
  </div>
<% end %>
