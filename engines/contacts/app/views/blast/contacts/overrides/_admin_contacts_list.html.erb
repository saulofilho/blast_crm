<div class="panel panel-primary">
  <table class="table">
    <thead>
      <th>ID</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Company</th>
      <th>Email</th>
      <th>Phone</th>
      <th></th>
    </thead>
    <tbody>
      <% @contacts.each do |contact| %>
        <tr>
          <td><%= contact.id %></td>
          <td><%= contact.first_name %></td>
          <td><%= contact.last_name %></td>
          <td><%= contact.company %></td>
          <td><%= contact.email %></td>
          <td><%= contact.phone %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
<br>
